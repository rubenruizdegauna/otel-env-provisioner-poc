extensions:
  health_check:

receivers:
  otlp:
    protocols:
      grpc:
      http:

processors:
  batch:
  resourcedetection:
    detectors: [ env, system ]

exporters:
  logging:
    logLevel: debug
  otlp:
    endpoint: otlp.nr-data.net:4317
    tls:
      insecure: true

service:
  pipelines:
    metrics:
      receivers: [ otlp ]
      processors: [ batch, resourcedetection ]
      exporters: [ logging, otlp ]

  extensions: [ health_check ]